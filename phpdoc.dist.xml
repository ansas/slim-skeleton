<?xml version="1.0" encoding="UTF-8"?>
<phpdoc>
    <title>Application</title>
    <parser>
        <target>docs</target>
        <default-package-name>Documentation</default-package-name>
        <markers>
            <item>TODO</item>
            <item>FIXME</item>
        </markers>
        <extensions>
            <extension>php</extension>
        </extensions>
    </parser>
    <transformer>
        <target>docs</target>
    </transformer>
    <transformations>
        <template name="responsive" />
    </transformations>
    <files>
        <directory>src</directory>
    </files>
</phpdoc>
